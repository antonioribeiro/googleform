    "description": "Post Data to Google Forms Spreadsheets",
    "keywords": [
        "pragmarx",
        "googleforms"
    ],
    "homepage": "https://github.com/antonioribeiro/googleforms",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Carlos Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "homepage": "http://antoniocarlosribeiro.com",
            "role": "Creator"
        }
    ],
    "require": {
        "php" : ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
